{
  "expo": {
    "name": "test",
    "slug": "test_project",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/rsz_appicon.png",
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android" : {
      "package": "com.angelchavez.app",
      "versionCode": 1,
      "permissions": ["LOCATION"],
      "icon" : "./assets/rsz_appicon.png"
    }
  }
}
